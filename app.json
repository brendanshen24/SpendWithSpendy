{
  "expo": {
    "name": "SpendWithSpendy",
    "slug": "SpendWithSpendy",
    "version": "1.0.0",
    "fonts": {
      "SF-Pro-Text-Regular": "./assets/fonts/SF-Pro-Text-Regular.woff",
      "SF-Pro-Text-Bold": "./assets/fonts/SF-Pro-Text-Bold.woff"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "react-native-nfc-manager",
        {
          "nfcPermission": "Allow this app to scan NFC tags to read their type and contents."
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "infoPlist": {
        "NFCReaderUsageDescription": "Allow this app to scan NFC tags to read their type and contents.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.nfc.readersession.formats": [
          "NDEF",
          "TAG",
          "NDEF",
          "TAG",
          "NDEF",
          "TAG",
          "NDEF",
          "TAG"
        ]
      },
      "bundleIdentifier": "com.anonymous.SpendWithSpendy"
    },
    "android": {
      "permissions": [
        "android.permission.NFC"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Surge.SpendWithSpendy"
    },
    "extra": {
      "eas": {
        "projectId": "25db5ca5-51d4-41d3-bfd0-937ec7dc9d1f"
      }
    },
    "developmentClient": true,
    "updates": {
      "enabled": false
    }
  }
}
